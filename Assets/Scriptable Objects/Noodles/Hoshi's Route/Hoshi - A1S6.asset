        classNameString: NoodlesNodeMultipleDialogueVisual, Assembly-CSharp, Version=0.0.0.0,
          Culture=neutral, PublicKeyToken=null
        dialogueStrips:
        - character: 0
          status: 0
          sentence: Yo tampoco pude hacer nada Hoshi, solo pude mirar...
        - character: 0
          status: 0
          sentence: Esas sombras son la culpa que sentimos por las acciones que cometimos
            en el pasado.
        - character: 0
          status: 0
          sentence: Creo que nadie en tu hogar te quisiera ver sufriendo.
        - character: 0
          status: 0
          sentence: "Hicimos lo que pudimos, pero no fue suficiente, pero nadie te
            lo va a recriminar jam\xE1s."
        - character: 0
          status: 0
          sentence: "Despu\xE9s de todo Hoshi, todos en ese pueblo te amaban, yo
            lo vi con mis propios ojos."
        - character: 0
          status: 0
          sentence: "Estoy seguro que ellos quer\xEDan que su estrella siga brillando
            a pesar de todo..."
        - character: 1
          status: 0
          sentence: P..., Gracias...
    0000001F:
      type: {class: NoodlesNodeMultipleDialogue, ns: , asm: Assembly-CSharp}
