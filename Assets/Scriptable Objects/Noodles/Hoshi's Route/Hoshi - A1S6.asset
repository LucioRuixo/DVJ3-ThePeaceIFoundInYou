        inputPorts:
        - GUID: f476593a-8f21-4837-a9fd-1db4fc4959b5
          parentNodeGUID: 415bb7f2-497e-4c69-9e72-979539bb88f3
          targetNodeGUID:
          - 90254751-3239-4bc7-8788-0d8d41e83ef7
