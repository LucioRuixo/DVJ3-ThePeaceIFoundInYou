        - GUID: d11dd25e-b4ca-4bdc-ae38-59401ab1fa28
          parentNodeGUID: a0190eb2-213a-4297-8361-3d60923f78be
          targetNodeGUID:
          - 2d218483-6fb7-47c7-943f-f92ad57d85b0
